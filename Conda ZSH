# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/soham/opt/anaconda3/bin/conda' 'shell.zsh' '$
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/soham/opt/anaconda3/etc/profile.d/conda.sh" ]; th$
        . "/Users/soham/opt/anaconda3/etc/profile.d/conda.sh"   
    else
        export PATH="/Users/soham/opt/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<
